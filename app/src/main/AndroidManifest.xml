<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="my.edu.um.fsktm.unihelp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.course.CourseRatingActivity"/>
        <activity android:name=".ui.course.CourseRatingFormActivity"/>
        <activity android:name=".ui.course.CourseGroupsActivity"/>
        <activity android:name=".ui.course.CourseDescriptionActivity"/>
        <activity android:name=".ui.course.CourseDetailsActivity"/>
        <activity android:name=".ui.course.CourseInstructorsActivity"/>
        <activity android:name=".ui.reservation.RoomDetailActivity"></activity>
        <activity
            android:name=".LoginActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>